doctype html
html(lang="es")
    head
        meta(charset="UTF-8")
        meta(http-equiv="X-UA-Compatible", content="IE=edge")
        meta(name="viewport", content="width=device-width, initial-scale=1.0")
        title=pagina
        link(href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/css/bootstrap.min.css", rel="stylesheet", integrity="sha384-Zenh87qX5JnK2Jl0vWa8Ck2rdkQ2Bzep5IDxbcnCeuOxjzrPF/et3URy9Bv1WTRi",crossorigin="anonymous")
    body
        div(class="container bg-light")
            div(class="shadow mt-2 row")
                div(class="col-5 mt-5 pt-5 pb-5 border-end")
                    .row
                        div(class="col-12 mb-5")
                            img(src="img/logo.png" class="img-fluid")

                        div(class="col-8 offset-2 d-grid gap-2 mb-4 mt-5")
                            a(class="btn btn-primary d-block mb-3" href="/clientes") clientes
                            a(class="btn btn-primary d-block" href="/productos") productos

                        div(class="col-8 offset-2 d-grid gap-2 mb-1")
                            h4 Creador: Lewis
                            p Programacion Avanzada 
                                span=ActualYear                     
                div(class="col-7 mt-5 pt-5 pb-5")
                    block contenido
    script(src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/js/bootstrap.bundle.min.js",integrity="sha384-OERcA2EqjJCMA+/3y+gxIOqMEjwtxJY7qPCqsdltbNJuaOe923+mo//f6V8Qbsw3", crossorigin="anonymous")